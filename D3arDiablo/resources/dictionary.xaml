<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ResourceDictionary.MergedDictionaries>
  </ResourceDictionary.MergedDictionaries>
  <Style x:Key="MainToolbarButton">
    <Style.Setters>
      <Setter Property="Control.Padding" Value="5"></Setter>
    </Style.Setters>
  </Style>
  <Style x:Key="ImageItem" TargetType="Image">
    <Style.Setters>
      <Setter Property="Width" Value="48"></Setter>
      <Setter Property="Height" Value="99"></Setter>
    </Style.Setters>
  </Style>
  <Style x:Key="BorderImageItem" TargetType="Border">
    <Style.Setters>
      <Setter Property="Border.BorderBrush" Value="Coral"></Setter>
      <Setter Property="Border.BorderThickness" Value="2"></Setter>
    </Style.Setters>
  </Style>
</ResourceDictionary>